<div class="container">
  <div class="row">

  <div class="trip-column-1">
    <h1 class="mt-5">Add an upcoming trip</h1>
    <%= simple_form_for @trip do |f| %>
    <%= f.association :city %>
    <%= f.input :start_date, as: :date, html5: true %>
    <%= f.input :end_date, as: :date, html5: true %>
    <%= f.button :submit, value: "Add a trip", class: "btn btn-dark" %>
    <% end %>
  </div>

  <div class="trip-column-2">

  </div>

  </div>


</div>
